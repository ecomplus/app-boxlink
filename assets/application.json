  },
  "auth_scope": {
    "stores/me": [
      "GET"
    ],
    "procedures": [
      "POST"
    ],
    "products": [],
    "brands": [],
    "categories": [],
    "customers": [],
    "orders": [],
    "carts": [],
    "orders/fulfillments": [],
    "orders/payments_history": [],
    "products/quantity": [],
    "products/variations/quantity": [],
    "products/price": [],
    "products/variations/price": []
  },
  "admin_settings": {
    "zip": {
      "schema": {
        "type": "string",
        "maxLength": 9,
        "pattern": "^[0-9]{5}-?[0-9]{3}$",
        "title": "CEP de origem"
      },
      "hide": true
    },
    "token": {
      "schema": {
        "type": "string",
        "maxLength": 256,
        "title": "Token boxlink"
      },
      "hide": true
    }
  },
  "auth_callback_uri": "https://us-central1-ecom-boxlink.cloudfunctions.net/app/ecom/auth-callback",
  "version": "1.0.0"
}